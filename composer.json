    "keywords": ["silex", "rest", "api"],
    "type": "library",
    "license": "free BSD license",
    "authors": [{
        "name": "newLoki",
        "email": "alberto.assmann@mayflower.de",
        "homepage": "newloki.de"
    }],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/event-dispatcher": "dev-master",
        "silex/silex": "dev-master",
        "symfony/class-loader": "2.1.*",
        "symfony/config": "2.1.*",
        "symfony/console": "2.1.*",
        "symfony/process": "2.1.*",
        "symfony/validator": "2.1.*",
        "symfony/yaml": "2.1.*",
        "symfony/browser-kit": "dev-master",
        "palma/DoctrineORMServiceProvider": "*"
